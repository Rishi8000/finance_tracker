{
    "version": 2,
    "builds": [
      {
        "src": "finance_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",
                    "runtime": "python3.10",
                    "buildCommand": "setup.sh" 
                  }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "finance_traker/wsgi.py"
      }
    ],
    "rewrites":[
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }