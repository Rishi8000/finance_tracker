{
    "version": 2,
    "builds": [
      {
        "src": "finance_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",
                    "runtime": "python3.10",
                    "buildCommand": "bash setup.sh" 
                  }
      }
    ],
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "finance_traker/wsgi.py"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      }
    ]
  }